@using EntityLayer.Concrete
@model List<Category>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Description</th>
        <th>Status</th>
        <th>Delete</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>@item.Status</td>
            <td><a href="#" class="btn btn-danger">Delete</a></td>
        </tr>
    }
</table>